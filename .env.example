# Copy this file to `.env` and fill in your credentials.

# Required for Tinker training runs.
TINKER_API_KEY=tml-your-key-here

# Optional but recommended for experiment tracking.
WANDB_API_KEY=your-wandb-api-key
WANDB_PROJECT=sdpo-chess
WANDB_ENTITY=your-wandb-entity
WANDB_MODE=online

# Optional runtime override. Leave unset unless you need a non-default endpoint.
# TINKER_BASE_URL=https://your-tinker-endpoint
STOCKFISH_PATH=/Users/vincent/.local/bin/stockfish
SYZYGY_PATH=/Users/vincent/.local/share/syzygy/standard
